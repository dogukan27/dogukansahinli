@model IEnumerable<YemekTarifleri.Identity.ApplicationUser>
@{
    ViewBag.Title = "List";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>List</h2>


    <div class="container">
        <table class="table">
            <tr>
                <th>
                   Name
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.UserName)
                    </td>
                </tr>
            }

        </table>
    </div>
