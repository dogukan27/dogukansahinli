@model tuning.Models.BasvuruSayfasi

@{
    ViewBag.Title = "Human Resources";
}




<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="icon" href="img/favicon.png" type="image/png">
    <title>Hp Chip Tuning │ North Cyprus </title>
    <link rel="stylesheet" href="~/Backend-Challenge/FrontEnd/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/Backend-Challenge/FrontEnd/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/Backend-Challenge/FrontEnd/vendors/icomoon-icon/style.css">
    <link rel="stylesheet" href="~/Backend-Challenge/FrontEnd/vendors/datetimepicker/tempusdominus-bootstrap-4.min.css">
    <link rel="stylesheet" href="~/Backend-Challenge/FrontEnd/vendors/nice-select/css/nice-select.css">
    <link rel="stylesheet" href="~/Backend-Challenge/FrontEnd/vendors/owl-carousel/assets/owl.carousel.min.css">
    <link rel="stylesheet" href="~/Backend-Challenge/FrontEnd/vendors/slick/slick.css">
    <link rel="stylesheet" href="~/Backend-Challenge/FrontEnd/vendors/slick/slick-theme.css">
    <link rel="stylesheet" href="~/Backend-Challenge/FrontEnd/vendors/popup/magnific-popup.css">
    <link rel="stylesheet" href="~/Backend-Challenge/FrontEnd/vendors/animate-css/animate.css">
    <link rel="stylesheet" href="~/Backend-Challenge/FrontEnd/css/style.css">
    <link rel="stylesheet" href="~/Backend-Challenge/FrontEnd/css/responsive.css">

    <style>
    </style>
</head>
<body data-scroll-animation="true" class="luxuary_bg">


    <div class="preloader">
        <div class="main-loader">
            <span class="loader1"></span>
            <span class="loader2"></span>
            <span class="loader3"></span>
        </div>
    </div>

    @Html.Partial("_header")


    <section class="breadcrumb_area" style=" background: url(/Backend-Challenge/FrontEnd/img/breadcrumb/breadcrumb-bg-3.jpg)">
        <div class="container">
            <div class="breadcrumb_inner mt-75">
                <h3>İnsan Kaynakları</h3>
                <div class="link">
                    <a href="index.html">Anasayfa</a>
                    <a href="#">İnsan Kaynakları</a>
                </div>
            </div>
        </div>
    </section>


    <section class="billing_details_area p_100">
        <div class="container">
            <div class="text-center single_dubble_title flag_title white slider_form_title mt-60 mb-50">
                <h5 class="main-red">Bizimle Çalışmak İster Misiniz ?</h5>
                <h2 class="wow  fadeInUp  animated" data-wow-delay="0.2s" style="visibility: visible; animation-delay: 0.2s; animation-name: fadeInUp;">
                    İnsan Kaynakları Formu
                </h2>
            </div>
            <div class="row">
                <div class="col-lg-7 mb-50 form-bg">
                    <div class="billing_form_area">

                        <form class="billing_form row" action="/Basvuru/HumanResources" enctype="multipart/form-data"
                              method="post" id="contactForm" novalidate="novalidate">
                            @Html.AntiForgeryToken()
                            <div class="form-group col-md-6">
                                @Html.LabelFor(model => model.Ad)
                                @Html.TextBoxFor(i => i.Ad, new { @class = "form-control" })
                            </div>
                            <div class="form-group col-md-6">
                                @Html.LabelFor(model => model.Soyad)
                                @Html.TextBoxFor(i => i.Soyad, new { @class = "form-control" })
                            </div>
                            <div class="form-group col-md-6">
                                @Html.LabelFor(model => model.Telefon)
                                @Html.TextBoxFor(i => i.Telefon, new { @class = "form-control" })
                            </div>
                            <div class="form-group col-md-6">
                                @Html.LabelFor(model => model.Email)
                                @Html.TextBoxFor(i => i.Email, new { @class = "form-control" })
                            </div>
                            <div class="form-group col-md-6">
                                @Html.LabelFor(model => model.Sehir)
                                @Html.DropDownListFor(model => model.Sehir, new SelectList(new List<string>() { "girne", "lefkoşa" }, "Şehir Girin"), new { @class = "nice-select w-100" })
                            </div>
                            <div class="form-group col-md-6">
                                @Html.LabelFor(model => model.PostaKodu)
                                @Html.TextBoxFor(i => i.PostaKodu, new { @class = "form-control" })
                            </div>

                            <div class="form-group col-md-12">
                                <label for="address">Adres <span>*</span></label>
                                @Html.TextBoxFor(i => i.Mahalle, new { @class = "form-control", @placeholder = "Mahalle ve Semt Adı" })
                                @Html.TextBoxFor(i => i.Apartman, new { @class = "form-control", @placeholder = "Apartman ve kapı numarası" })
                            </div>
                            <div class="form-group col-md-12">
                                <label for="phone">HpChipTuning ailesine neden katılmak istersiniz ? </label>
                                @Html.TextBoxFor(i => i.Tanıtım, new { @class = "form-control", @placeholder = "Kendinizi kısaca tanıtınız" })

                            </div>
                            <div class="form-group col-md-6  ">
                                <label for="cv">Cv Yükleyin</label>
                                <input type="file" class="form-control-file cv-back" id="cv" name="cv">
                            </div>
                            <div class="form-group col-md-6 cv-btn">
                                <button type="submit" value="submit" class="btn submit_btn red form-control">Gönder</button>

                            </div>
                        </form>
                    </div>
                </div>
                <div class="col-lg-5">
                    <div class="order_box_price ">
                        <img src="~/Backend-Challenge/FrontEnd/img/human-resources.jpg" alt="" class="img-fluid w-100 ">
                    </div>
                </div>
            </div>
        </div>
    </section>


    <footer class="footer_area red_footer_area ab_footer ">
        <div class="footer_widgets">
            <section class="client_logo_slider_area border_none">
                <div class="container">
                    <div class="client_logo_slider owl-carousel">
                        <div class="item">
                            <img src="~/Backend-Challenge/FrontEnd/img/client-logo/elnec.png" alt="">
                        </div>
                        <div class="item">
                            <img src="~/Backend-Challenge/FrontEnd/img/client-logo/evc.png" alt="">
                        </div>
                        <div class="item">
                            <img src="~/Backend-Challenge/FrontEnd/img/client-logo/race.png" alt="">
                        </div>
                        <div class="item">
                            <img src="~/Backend-Challenge/FrontEnd/img/client-logo/fg.png" alt="">
                        </div>
                        <div class="item">
                            <img src="~/Backend-Challenge/FrontEnd/img/client-logo/cmd.png" alt="">
                        </div>
                        <div class="item">
                            <img src="~/Backend-Challenge/FrontEnd/img/client-logo/piasini.png" alt="">
                        </div>
                        <div class="item">
                            <img src="~/Backend-Challenge/FrontEnd/img/client-logo/alientech.png" alt="">
                        </div>
                        <div class="item">
                            <img src="~/Backend-Challenge/FrontEnd/img/client-logo/byte.png" alt="">
                        </div>
                    </div>
                </div>
            </section>
            <div class="container">
                <div class="row f_widgets_inner">
                    <div class="col-lg-3 col-sm-6 wow animated fadeInUp" data-wow-delay="0.2s">
                        <aside class="f_widget about_widget">
                            <a class="f_logo" href="#">
                                <img src="~/Backend-Challenge/FrontEnd/img/repair-logo.png"
                                     srcset="~/Backend-Challenge/FrontEnd/img/chip-logo2.png 2x" alt="bar" class="max-width">
                            </a>
                            <p>
                                <i class="icon-map_marker_2" aria-hidden="true"></i> Larnaka Bul. Köseoğlu Center No:2-a
                                İskele / KIBRIS
                            </p>
                            <a href="mailto:info@hpchiptuning.com">
                                <i class="icon-envelop_2" aria-hidden="true"></i>
                                info@hpchiptuning.com
                            </a>
                            <a href="tel:05338309600"><i class="icon-phone_2" aria-hidden="true"></i> +90 533 830 96 00</a>
                        </aside>
                    </div>
                    <div class="col-lg-2 col-sm-6 wow animated fadeInUp" data-wow-delay="0.4s">
                        <aside class="f_widget link_widget">
                            <div class="f_title">
                                <h3>Chip Tuning</h3>
                            </div>
                            <ul class="nav flex-column">
                                <li><a href="#">Güç Hesaplama</a></li>
                                <li><a href="#">Tuning Hizmetleri</a></li>
                                <li><a href="#">Tuning Haberler</a></li>
                                <li><a href="#">İletişim</a></li>
                            </ul>
                        </aside>
                    </div>
                    <div class="col-lg-2 col-sm-6 wow animated fadeInUp" data-wow-delay="0.6s">
                        <aside class="f_widget link_widget">
                            <div class="f_title">
                                <h3>Hızlı Menü</h3>
                            </div>
                            <ul class="nav flex-column">
                                <li><a href="#">Anasayfa</a></li>
                                <li><a href="#">Hakkımızda</a></li>
                                <li><a href="#">Kurumsal</a></li>
                                <li><a href="#">S.S.S</a></li>
                            </ul>
                        </aside>
                    </div>
                    <div class="col-lg-5 col-sm-6 wow animated fadeInUp" data-wow-delay="0.8s">
                    </div>
                </div>
            </div>
        </div>


        <div class="footer_card wow animated fadeIn" data-wow-delay="0.4s">
            <div class="container">
                <div class="f_card_inner d-flex justify-content-between">
                    <div class="left">
                        <p class="mt-10 color-white">
                            Sosyal medya hesaplarımızı takip ederek araçlar hakkında anında bilgi
                            sahibi olabilirsiniz.
                        </p>
                    </div>
                    <aside class="f_widget subscribe_widget">
                        <div class="right">
                            <ul class="nav">
                                <li><a href="#"><i class="fa fa-facebook"></i></a></li>
                                <li><a href="#"><i class="fa fa-youtube"></i></a></li>
                                <li><a href="#"><i class="fa fa-twitter"></i></a></li>
                                <li><a href="#"><i class="fa fa-instagram"></i></a></li>
                            </ul>
                        </div>
                    </aside>
                </div>
            </div>
        </div>
        <div class="footer_copyright">
            <div class="container">
                <div class="copyright_inner d-flex justify-content-between">
                    <div class="left">
                        <p>
                            Copyright
                            <script>document.write(new Date().getFullYear());</script>
                            HpChipTuning

                        </p>
                    </div>
                    <div class="right">
                        <ul class="nav">
                            <li><a href="#"><img src="~/Backend-Challenge/FrontEnd/img/hashlogo.png" alt=""></a></li>

                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </footer>


    <script src="~/Backend-Challenge/FrontEnd/js/jquery-3.4.1.min.js"></script>
    <script src="~/Backend-Challenge/FrontEnd/js/popper.min.js"></script>
    <script src="~/Backend-Challenge/FrontEnd/js/bootstrap.min.js"></script>
    <script src="~/Backend-Challenge/FrontEnd/vendors/datetimepicker/moment.js"></script>
    <script src="~/Backend-Challenge/FrontEnd/vendors/datetimepicker/tempusdominus-bootstrap-4.min.js"></script>
    <script src="~/Backend-Challenge/FrontEnd/vendors/nice-select/js/jquery.nice-select.min.js"></script>
    <script src="~/Backend-Challenge/FrontEnd/vendors/owl-carousel/owl.carousel.min.js"></script>
    <script src="~/Backend-Challenge/FrontEnd/vendors/isotope/imagesloaded.pkgd.min.js"></script>
    <script src="~/Backend-Challenge/FrontEnd/vendors/isotope/isotope.pkgd.min.js"></script>
    <script src="~/Backend-Challenge/FrontEnd/vendors/slick/slick.min.js"></script>
    <script src="~/Backend-Challenge/FrontEnd/vendors/counter-up/jquery.waypoints.min.js"></script>
    <script src="~/Backend-Challenge/FrontEnd/vendors/counter-up/jquery.counterup.min.js"></script>
    <script src="~/Backend-Challenge/FrontEnd/vendors/popup/jquery.magnific-popup.min.js"></script>
    <script src="~/Backend-Challenge/FrontEnd/vendors/animate-css/wow.min.js"></script>
    <script src="~/Backend-Challenge/FrontEnd/js/theme-dist.js"></script>
    <script src="~/Backend-Challenge/FrontEnd/js/main.js"></script>

</body>
</html>








